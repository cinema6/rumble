    {
        "id": "e-55b499581cb19e",
        "uri": "wrestling",
        "appUriPrefix": "<%= settings.appUrl %>",
        "appUri": "rumble",
        "title": "The 7 Most Legendary Pro Wrestling Intros Of All Time",
        "subtitle": "",
        "summary": "Nothing electrifies a crowd like a good wrestling entrance. Finishing moves are cool, but most of them don't even hurt!",
